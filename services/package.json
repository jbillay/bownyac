{
  "name": "bownyac-services",
  "version": "0.0.1",
  "description": "Services for the new application",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/jbillay/bownyac.git"
  },
  "scripts": {
    "start": "nodemon index.js",
    "test": "jest --forceExit",
    "lint": "./node_modules/.bin/eslint **/*.js"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/test/**/*.test.js"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!index.js",
      "!**/node_modules/**",
      "!**/config/**",
      "!**/.config/**",
      "!**/coverage/**",
      "!**/test/**"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "html"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "docs/*",
      "node_modules/**/node_modules"
    ],
    "delay": "2500"
  },
  "author": "Jeremy Billay",
  "license": "MIT",
  "dependencies": {
    "@koa/cors": "^2.2.1",
    "debug": "^3.1.0",
    "jsonwebtoken": "^8.2.0",
    "koa": "^2.4.1",
    "koa-bodyparser": "^4.2.0",
    "koa-compress": "^2.0.0",
    "koa-conditional-get": "^2.0.0",
    "koa-etag": "^3.0.0",
    "koa-generic-session": "^2.0.1",
    "koa-helmet": "^4.0.0",
    "koa-jwt": "^3.3.1",
    "koa-redis": "^3.1.2",
    "koa-respond": "^1.0.1",
    "koa-route": "^3.2.0",
    "koa-router": "^7.4.0",
    "mongodb": "^3.0.1",
    "mongoose": "^5.0.6",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "apidoc": "^0.17.6",
    "eslint": "^4.18.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "jest": "^22.3.0",
    "nodemon": "^1.15.1",
    "supertest": "^3.0.0"
  }
}
